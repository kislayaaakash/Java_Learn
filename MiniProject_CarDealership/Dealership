package car_dealership;

public class Dealership {

	public static void main(String[] args) {
		
		Customer cust1 = new Customer("Tom", "Apt 123 Square Block, Hudson Street, NY - 10001",12000);
		
		Vehicle vehicle = new Vehicle("Honda","S-Series",15000,"Honda civic");
		
		Employee emp = new Employee();
		
		cust1.purchaseCar(vehicle , emp, true);

	}

}
