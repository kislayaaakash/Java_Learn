public static int countVowels(String str)
    {
        int count = 0;
        HashSet<Character> hs = new HashSet<>();
        for(int i =0; i < str.length(); i++){
            hs.add(str.charAt(i));
        }
        for(char c : hs){
        if(c == 'a' || c == 'e' || c == 'i' || c == 'o' || c == 'u')
            count++;
        }
        return count;
    }
